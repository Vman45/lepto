{
  "name": "lepto",
  "version": "1.1.1",
  "description": "Advanced image Editing, Optimization and Analysis CLI and GUI tool",
  "main": "src/lepto.js",
  "author": "Dimitri NICOLAS <dimitri@fivecorp.fr>",
  "license": "MIT",
  "homepage": "https://github.com/dimitrinicolas/lepto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimitrinicolas/lepto.git"
  },
  "bugs": {
    "url": "https://github.com/dimitrinicolas/lepto/issues"
  },
  "keywords": [
    "front-end",
    "images",
    "optimization",
    "javascript",
    "jpg",
    "webpg"
  ],
  "watch": {
    "lint": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "js"
    },
    "style:build": {
      "patterns": [
        "src/gui/src"
      ],
      "extensions": "css"
    },
    "script:build": {
      "patterns": [
        "src/gui/src"
      ],
      "extensions": "js"
    }
  },
  "dependencies": {
    "chokidar": "^2.0.4",
    "deep-object-diff": "^1.1.0",
    "directory-tree": "^2.1.0",
    "exec-buffer": "^3.2.0",
    "fast-glob": "^2.2.2",
    "file-type": "^8.1.0",
    "fs-extra": "^7.0.0",
    "gifsicle": "^3.0.4",
    "image-size": "^0.6.3",
    "minimatch": "^3.0.4",
    "opn": "^5.3.0",
    "pngquant": "^1.3.0",
    "sharp": "^0.20.5",
    "socket.io": "^2.1.1",
    "svgo": "^1.0.5"
  }
}
